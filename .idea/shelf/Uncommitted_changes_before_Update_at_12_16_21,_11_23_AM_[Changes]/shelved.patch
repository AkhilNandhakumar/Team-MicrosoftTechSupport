<+>{% extends \"assignments/AboutUs/about_layout.html\" %}\n{% set project = \"Yash About Me\" %}\n\n<script>\n    let imgStatus = 'mask';\n    function imageToggle() {\n        if (imgStatus === 'mask') {\n            let imgType = document.getElementById(\"toggleImage\");\n            imgType.src = \"/Static/AboutUs-Pictures/Yash/LinkedIn-Profile.jpeg\";\n            imgStatus = 'nomask';\n        }\n        else{\n\n            let imgType = document.getElementById(\"toggleImage\");\n            imgType.src = \"/Static/AboutUs-Pictures/Yash/CSP PIC2.jpeg\";\n            imgStatus = 'mask';\n        }\n    }\n    let slideIndex = 1;\n    showSlides(slideIndex);\n\n    // Next/previous controls\n    function plusSlides(n) {\n        showSlides(slideIndex += n);\n    }\n\n    // Thumbnail image controls\n    function currentSlide(n) {\n        showSlides(slideIndex = n);\n    }\n\n    function showSlides(n) {\n        let i;\n        let slides = document.getElementsByClassName(\"mySlides\");\n        let dots = document.getElementsByClassName(\"dot\");\n        if (n > slides.length) {slideIndex = 1}\n        if (n < 1) {slideIndex = slides.length}\n        for (i = 0; i < slides.length; i++) {\n            slides[i].style.display = \"none\";\n        }\n        for (i = 0; i < dots.length; i++) {\n            dots[i].className = dots[i].className.replace(\" active\", \"\");\n        }\n        slides[slideIndex-1].style.display = \"block\";\n        dots[slideIndex-1].className += \" active\";\n    }\n    function showSlides() {\n        let i;\n        let slides = document.getElementsByClassName(\"mySlides\");\n        for (i = 0; i < slides.length; i++) {\n            slides[i].style.display = \"none\";\n        }\n        slideIndex++;\n        if (slideIndex > slides.length) {slideIndex = 1}\n        slides[slideIndex-1].style.display = \"block\";\n        setTimeout(showSlides, 2000); // Change image every 2 seconds\n    }\n</script>\n\n{% block body %}\n\n<style>\n    table {\n        width: 50%;\n        border: 1px solid white;\n    }\n    th, td, tr {\n        border: 1px solid white;\n        padding: 15px;\n        text-align: center;\n    }\n    img {\n        border: 5px solid aquamarine;\n    }\n    * {box-sizing:border-box}\n\n    /* Slideshow container */\n    .slideshow-container {\n        max-width: 1000px;\n        position: relative;\n        margin: auto;\n    }\n\n    /* Hide the images by default */\n    .mySlides {\n        display: none;\n    }\n\n    /* Next & previous buttons */\n    .prev, .next {\n        cursor: pointer;\n        position: absolute;\n        top: 50%;\n        width: auto;\n        margin-top: -22px;\n        padding: 16px;\n        color: white;\n        font-weight: bold;\n        font-size: 18px;\n        transition: 0.6s ease;\n        border-radius: 0 3px 3px 0;\n        user-select: none;\n    }\n\n    /* Position the \"next button\" to the right */\n    .next {\n        right: 0;\n        border-radius: 3px 0 0 3px;\n    }\n\n    /* On hover, add a black background color with a little bit see-through */\n    .prev:hover, .next:hover {\n        background-color: rgba(0,0,0,0.8);\n    }\n\n    /* Caption text */\n    .text {\n        color: #f2f2f2;\n        font-size: 15px;\n        padding: 8px 12px;\n        position: absolute;\n        bottom: 8px;\n        width: 100%;\n        text-align: center;\n    }\n\n    /* Number text (1/3 etc) */\n    .numbertext {\n        color: #f2f2f2;\n        font-size: 12px;\n        padding: 8px 12px;\n        position: absolute;\n        top: 0;\n    }\n\n    /* The dots/bullets/indicators */\n    .dot {\n        cursor: pointer;\n        height: 15px;\n        width: 15px;\n        margin: 0 2px;\n        background-color: #bbb;\n        border-radius: 50%;\n        display: inline-block;\n        transition: background-color 0.6s ease;\n    }\n\n    .active, .dot:hover {\n        background-color: #717171;\n    }\n\n    /* Fading animation */\n    .fade {\n        -webkit-animation-name: fade;\n        -webkit-animation-duration: 1.5s;\n        animation-name: fade;\n        animation-duration: 1.5s;\n    }\n\n    @-webkit-keyframes fade {\n        from {opacity: .4}\n        to {opacity: 1}\n    }\n\n    @keyframes fade {\n        from {opacity: .4}\n        to {opacity: 1}\n    }\n</style>\n\n<div style=\"text-align: center;\">\n    <h1>Yash Shah</h1>\n    <span>\n        <p>My name is Yash Shah. I am 16 years old, and I am junior at Del Norte High School.</p>\n        <p>Some of my hobbies include spending time with my family, hanging out with friends, building LEGOs, photography, and of course, sleeping.</p>\n        <p>I would like to work as an artificial intelligence programmer or work in the private equity sector.</p>\n\n        <div align=\"center\">\n            <a href=\"https://docs.google.com/document/d/1b8XxDljHwsNPgyQ3KBtOjHy1575NbxJuUtkxBfm_Hk0/edit\">CSP Journal</a><br>\n            <a href=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/commits?author=YashShah138\">Commits</a><br>\n            <a href=\"https://docs.google.com/document/d/14YG7ZscxqKMrEArx1GuzEqcIB_f6ZqT086UE8ttQ3lM/edit?usp=sharing\">Create Task</a>\n            <br>\n                <img id=\"toggleImage\" src=\"/static/AboutUs-Pictures/Yash/LinkedIn-Profile.jpeg\" alt=\"\" width=\"225\" height=\"300\">\n            <br>\n            <br>\n                <button id=\"toggleButton\" onclick=\"imageToggle()\">Click to toggle image!</button>\n        </div>\n    </span>\n</div>\n<br>\n<div class=\"slideshow-container\">\n    <div class=\"mySlides fade\">\n        <img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/Sunset1.jpeg?raw=true\" alt=\"\" width=\"300\" height=\"500\">\n        <div class=\"text\">Sunset over UCSD Pier</div>\n    </div>\n    <div class=\"mySlides fade\">\n        <img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/Carasauel1.jpeg?raw=true\" alt=\"\" width=\"300\" height=\"500\">\n        <div class=\"text\">Coast Along Highway 1</div>\n    </div>\n    <div class=\"mySlides fade\">\n        <img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c2.jpeg?raw=true\" alt=\"\" width=\"300\" height=\"500\">\n        <div class=\"text\">Lake in the trees</div>\n    </div>\n    <div class=\"mySlides fade\">\n        <img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c2.jpeg?raw=true\" alt=\"\" width=\"500\" height=\"300\">\n        <div class=\"text\">Lake in the trees</div>\n    </div>\n        <figure><img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c2.jpeg?raw=true\" alt=\"Lake in the Trees\" width=\"500\" height=\"300\"></figure>\n        <figure><img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c3.jpeg?raw=true\" alt=\"Sun in the Dead\" width=\"500\" height=\"300\"></figure>\n        <figure><img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c4.jpeg?raw=true\" alt=\"The Star\" width=\"300\" height=\"500\"></figure>\n        <figure><img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c5.jpeg?raw=true\" alt=\"La Jolla Sunset\" width=\"500\" height=\"300\"></figure>\n        <figure><img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c6.jpeg?raw=true\" alt=\"Train Through The Woods\" width=\"500\" height=\"300\"></figure>\n        <figure><img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c7.jpeg?raw=true\" alt=\"Tired Lioness\" width=\"500\" height=\"300\"></figure>\n        <figure><img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c8.jpeg?raw=true\" alt=\"Dark City\" width=\"500\" height=\"300\"></figure>\n        <figure><img src=\"https://github.com/AkhilNandhakumar/Team-MicrosoftTechSupport/blob/main/Static/AboutUs-Pictures/Yash/c9.jpeg?raw=true\" alt=\"University of Texas\" width=\"500\" height=\"300\"></figure>\n    <br>\n    <br>\n</div>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n<br>\n\n<div style=\"text-align: center;\">\n    <h3>Formula One Constructors</h3>\n    <table align=\"center\">\n        <tr>\n            <th>Team</th>\n            <th>Name</th>\n            <th>Country</th>\n            <th>Number of Podiums</th>\n        </tr>\n        <tr>\n            <td>{{ data[0].team }}</td>\n            <td>{{ data[0].name }}</td>\n            <td>{{ data[0].country }}</td>\n            <td>{{ data[0].podiums }}</td>\n        </tr>\n        <tr>\n            <td>{{ data[1].team }}</td>\n            <td>{{ data[1].name }}</td>\n            <td>{{ data[1].country }}</td>\n            <td>{{ data[1].podiums }}</td>\n        </tr>\n        <tr>\n            <td>{{ data[2].team }}</td>\n            <td>{{ data[2].name }}</td>\n            <td>{{ data[2].country }}</td>\n            <td>{{ data[2].podiums }}</td>\n        </tr>\n        <tr>\n            <td>{{ data[3].team }}</td>\n            <td>{{ data[3].name }}</td>\n            <td>{{ data[3].country }}</td>\n            <td>{{ data[3].podiums }}</td>\n        </tr>\n        <tr>\n            <td>{{ data[4].team }}</td>\n            <td>{{ data[4].name }}</td>\n            <td>{{ data[4].country }}</td>\n            <td>{{ data[4].podiums }}</td>\n        </tr>\n        <tr>\n            <td>{{ data[5].team }}</td>\n            <td>{{ data[5].name }}</td>\n